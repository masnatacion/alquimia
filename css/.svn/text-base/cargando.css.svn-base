#cargando-mask {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 20002;
    background: #F0F0F0;
    background: #01527f;
    display: none;
}
#cargando {
    position: absolute;
    left: 50%;
    top: 50%;
    padding: 2px;
    z-index: 20001;
    height: auto;
    margin: -100px 0px 0px -100px;
}
#cargando .cargando-indicator {
    color: #dedede;
    text-align: center;
    height: auto;
    font-family: Arial;
}
#cargando-msg {

    font-size: 14px;
}

#cargando-mask div.imagen {
    background: url('../img/sprite.png');
    width: 206px;
    height: 65px;
    cursor: pointer;
    float: left;
    margin-bottom: 10px;
}

#cargando-mask {
    display: block;
}