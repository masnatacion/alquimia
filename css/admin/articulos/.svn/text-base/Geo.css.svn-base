.geolocalizacion{
    padding: 5px 10px;
    white-space: normal;
    color: #555;
    text-decoration: none;
    display: block;
    overflow: hidden;
}

.geolocalizacion div.contenido,
.geolocalizacion div.imagen{
    float: left;
}

.geolocalizacion div.imagen{
    margin: 0px 10px 0px 0px;
}

.geolocalizacion div.contenido{
    word-wrap: break-word;
    width: 80%;
}

.geolocalizacion div.contenido p{
    margin: 0px;
}