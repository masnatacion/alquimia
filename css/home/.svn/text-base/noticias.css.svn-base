.noticias{
  overflow: hidden;
  position: relative;
  width: 628px;
  margin:0 auto 8px 0px;
  background: #EAEAEA;
}

.noticias .royalSlider{
    width: 628px;
}

.noticias .royalSlider .rsNav{
    background-color: #0776AF;
    width: 628px;
    position: relative;
    z-index: 100;
}
    
.noticias .royalSlider .rsTmb
{
    float: left;
    cursor: pointer;
}

.noticias .royalSlider .rsNav span.rsTmb.titulo{
    font-weight: bold;
}

.noticias .royalSlider .rsNav span.rsTmb{
    color: white;
    font-size: 14px;
    text-decoration: none;
    padding: 7px 8px 6px 8px;
    display: block;
    text-shadow: 1px 1px 1px rgba(0,0,0,0.5);
}


.noticias .royalSlider .rsNav span.rsTmb:hover{
    background: white;
    color: #737373;
    text-shadow: none;
    padding: 6px 8px 5px 8px;
    margin: 1px 0px 1px 0px;
    cursor: pointer;
}

.noticias .royalSlider .rsNav .rsNavSelected span.rsTmb:hover,
.noticias .royalSlider .rsNav .rsNavSelected span.rsTmb{
    background-color: #06557D; 
    text-shadow: 1px 1px 1px rgba(0,0,0,0.5);
    color: white;
}

.noticias .royalSlider li.tab  ul li p{
  color: #636363;
  font-size: 17px;
  line-height: 18px;
  height: 57px;
  overflow: hidden;
}

.noticias .royalSlider li.tab  ul li h3{
    color: #0776AF;
    font-size: 16px;
    height: 40px;
    /*text-shadow: 0px 0px 1px rgba(0,0,0,0.5);*/
    font-weight: bold;
    margin: 6px 0px 8px 0px;
    text-transform:uppercase;
    /*1 line*/
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
}
.noticias .royalSlider li.tab  ul li{
    padding: 10px;
    overflow: hidden;
}

.noticias .royalSlider li.tab ul li:hover{
    background: white;
}

.noticias .royalSlider li.tab ul.big li{
    height: 407px;
}

.noticias .royalSlider li.tab  ul.big li{
    margin-bottom: 1px;
}


.noticias .royalSlider li.tab ul.small li:first-child{
    margin: 0px 1px 0px 0px;
}

.noticias .royalSlider li.tab ul.small li{
    margin: 8px 1px 0px 0px;
    height: 190px;
}


.noticias .royalSlider li.tab ul.big li img{
    width: 402px;
    height: 275px;
}
.noticias .royalSlider li.tab ul.small li img{
    width: 185px;
    height: 95px;
}

.noticias .royalSlider li.tab ul.small li:nth-child(3){
    margin: 1px 1px 0px 0px;
}

.noticias .royalSlider li.tab ul.small li:nth-child(2){
    margin: 7px 1px 0px 0px;
}

.noticias .royalSlider li.tab ul.small li:last-child{
    margin: 7px 1px 0px 0px;
}

.noticias .royalSlider li.tab  ul.small li p{
    overflow: hidden;
    height: 20px;
    font-size: 14px;
    margin: auto;
    color: #636363;
    line-height: normal;
}

.ie .noticias .royalSlider li.tab  ul.small li p{
    height: 34px;
}

.noticias .royalSlider li.tab  ul.small li h3{
    margin: 5px 0px 0px 0px;
    font-weight: bold;
    font-size: 14px;
}


.noticias .royalSlider li.tab  ul li div.categoria{
    text-shadow: 1px 1px 1px rgba(0,0,0,0.5);
    font-weight: bold;
    text-transform: uppercase;
    color: white;
    background: #E36A24;
    padding: 3px 8px 3px 8px;
    width: auto;
    font-size: 11px;
    overflow: hidden;
    position: relative;
    z-index: 1;
    float: right;
    margin-bottom: -21px;
}

.noticias .royalSlider li.tab  ul li div.categoria.natacion{
    background:#E56C26;
}

.noticias .royalSlider li.tab  ul li div.categoria.sincronizado{
    background:#581B4B;
}

.noticias .royalSlider li.tab  ul li div.categoria.ciclismo{
    background:#C6CA27;
}

.noticias .royalSlider li.tab  ul li div.categoria.triatlon{
    background:#73B763;
}

.noticias .royalSlider li.tab  ul li div.categoria.waterpolo{
    background:#C655A2;
}

.noticias .royalSlider li.tab  ul li div.categoria.clavados{
    background:#02A6D1;
}

.noticias .royalSlider li.tab  ul li i.icono{
    display: block;
    background: url('../../img/sprite.png');
    background-position: -220px -68px;
    height: 25px;
    width: 38px;
    position: relative;
    float: right;
    margin-bottom: -21px;
    bottom: 50px;
    right: 5px;
}

.noticias .royalSlider li.tab  ul li i.icono.galeria{
    background-position: -220px -68px;
}

.noticias .royalSlider li.tab  ul li i.icono.video{
    background-position: -270px -68px;
}


.noticias .royalSlider li.tab ul{
    float: left;
    overflow: hidden;
}

.noticias .royalSlider li.tab ul.big{
    width: 422px;
    margin-left: 1px;
}

.noticias .royalSlider li.tab ul.small{
    width: 205px;
}


.noticias .royalSlider .rsNav span.rsTmb i.icon.arrow:before{
    content: "\e011";
    color: #00689c;
}

.noticias .royalSlider .rsNav .rsNavSelected span.rsTmb i.icon.arrow:before{
    content: "\e037";
    color: #00476e;
}


.noticias .royalSlider li.tab li .social .num,
.noticias .royalSlider li.tab li .social .social-stub{
    margin: 7px 56px 0px 0px;
    float: left;
}

.noticias .royalSlider li.tab li:hover .social .num,
.noticias .royalSlider li.tab li .social .social-stub{
    display: none;
}

.noticias .royalSlider li.tab li .social .num,
.noticias .royalSlider li.tab li:hover .social .social-stub{
    display: block;
}


/*
MOBILE
*/

.mobile .noticias .royalSlider li.tab ul.big,
.mobile .noticias .royalSlider li.tab ul.small,
.mobile .noticias,
.mobile .noticias .royalSlider{
    min-width: 320px;
    width: 100%;
}

.mobile .noticias .royalSlider li.tab ul li i.icono,
.mobile .noticias .royalSlider li.tab ul li div.categoria,
.mobile .noticias .royalSlider li.tab ul.small li:nth-child(n+3),
.mobile .noticias .royalSlider li.tab li .social{
    display: none;
}

.mobile .noticias > .royalSlider .rsNav{
    overflow-x:auto;
    overflow-y:hidden;
    height: 32px;
}

.mobile .noticias .royalSlider .rsNav span.rsTmb{
    font-size: 12px;
    padding: 7px 6px 6px 6px;
}

.mobile .noticias .royalSlider li.tab ul.big li img,
.mobile .noticias .royalSlider li.tab ul.small li img{
    width: 120px;
    height: 80px;
    float: left;
}

.mobile .noticias .royalSlider li.tab ul.small li,
.mobile .noticias .royalSlider li.tab ul.big li{
    height: auto;
    padding: 10px 6px 10px 6px;
    margin: 1px 1px 1px 1px;
    width: 100%;
}

.mobile .noticias .royalSlider li.tab ul li div.contenido{
    position: absolute;
    margin: 0px 0px 0px 122px;
}

.mobile .noticias .royalSlider li.tab ul.small li h3,
.mobile .noticias .royalSlider li.tab li h3{
    margin: 0px 0px 0px 8px;
    font-size: 13px;
    color: #555555;
    height: 80px;
    text-shadow: 0px 0px 1px rgba(0,0,0,0.5);
    text-transform: none;
    font-weight: normal;
}

.mobile .noticias .royalSlider li.tab ul li p{
  margin: 0px 0px 0px 8px;
  line-height: normal;
  height: 51px;
  overflow: hidden;
  font-size: 12px;
  display: none;
  height: 40px;
  overflow: hidden;
}

.mobile .noticias .royalSlider li.tab ul.big li:first-child{
  padding: 0px;
  margin: 0px;
  position: relative;
  left: 0px;
}

.mobile .noticias .royalSlider li.tab ul.big li:first-child img {
  width: 100%;
  height: auto;
}

.mobile .noticias .royalSlider li.tab ul.big li:first-child div.contenido {
  overflow: hidden;
  clear: both;
  padding: 10px 10px 10px 10px;
  margin: 0px;
  bottom: 0px;
  height: 60px;
}

.mobile .noticias .royalSlider li.tab ul.big li:first-child h3{
  color: white;
  font-family: Ubuntu;
  text-decoration: none;
  z-index: 1;
  text-shadow: 4px 4px 4px rgba(0,0,0,0.5);
  font-weight: bold;
  font-size: 17px;
  margin: 5px 10px 2px 0px;
  line-height: 17px;
}

.mobile .noticias .royalSlider li.tab ul.big li:first-child span {
  height: 80px;
  width: 100%;
  position: absolute;
  bottom: 0px;
  margin: 0px;
  background-color: #020202;
  opacity: 0.6;
  display: block;
}

.mobile .noticias .royalSlider li.tab ul.big li:first-child p {
  color: white;
  z-index: 1;
  text-decoration: none;
  bottom: 0px;
  margin: 0px;
  opacity: 0.8;
  height: 35px;
  overflow: hidden;
}

.mobile .noticias .royalSlider li.tab ul.big {
  margin-left: 0px;
}