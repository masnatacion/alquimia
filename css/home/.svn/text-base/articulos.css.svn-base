.articulos{
  overflow: hidden;
  margin: 0px auto 15px auto;
  background: #EAEAEA;
  width: 100%;
}

.articulos .royalSlider{
  width: 100%;
}

.articulos .royalSlider .rsNav{
    background-color: #C655A2;
    width: 944px;
    position: relative;
    z-index: 100;
}
    
.articulos .royalSlider .rsTmb
{
    float: left;
    cursor: pointer;
}

.articulos .royalSlider .rsNav span.rsTmb.titulo{
    font-weight: bold;
}


.articulos .royalSlider li.tab  ul li div.categoria{
    text-shadow: 1px 1px 1px rgba(0,0,0,0.5);
    font-weight: bold;
    text-transform: uppercase;
    color: white;
    background: #E36A24;
    padding: 3px 8px 3px 8px;
    width: auto;
    font-size: 11px;
    overflow: hidden;
    position: relative;
    z-index: 1;
    float: right;
    margin-bottom: -21px;
}

.articulos .royalSlider li.tab  ul li div.categoria.natacion{
    background:#E56C26;
}

.articulos .royalSlider li.tab  ul li div.categoria.sincronizado{
    background:#581B4B;
}

.articulos .royalSlider li.tab  ul li div.categoria.ciclismo{
    background:#C6CA27;
}

.articulos .royalSlider li.tab  ul li div.categoria.triatlon{
    background:#73B763;
}

.articulos .royalSlider li.tab  ul li div.categoria.waterpolo{
    background:#C655A2;
}

.articulos .royalSlider li.tab  ul li div.categoria.clavados{
    background:#02A6D1;
}

.articulos .royalSlider li.tab  ul li i.icono{
    display: block;
    background: url('../../img/sprite.png');
    background-position: -220px -68px;
    height: 25px;
    width: 38px;
    position: relative;
    float: right;
    margin-bottom: -21px;
    bottom: 50px;
    right: 5px;
}

.articulos .royalSlider li.tab  ul li i.icono.galeria{
    background-position: -220px -68px;
}

.articulos .royalSlider li.tab  ul li i.icono.video{
    background-position: -270px -68px;
}

.articulos .royalSlider .rsNav span.rsTmb{
    color: white;
    font-size: 14px;
    text-decoration: none;
    padding: 7px 8px 6px 8px;
    display: block;
    text-shadow: 1px 1px 1px rgba(0,0,0,0.5);
}


.articulos .royalSlider .rsNav span.rsTmb:hover{
    background: white;
    color: #737373;
    text-shadow: none;
    padding: 6px 8px 5px 8px;
    margin: 1px 0px 1px 0px;
    cursor: pointer;
}

.articulos .royalSlider .rsNav .rsNavSelected span.rsTmb:hover,
.articulos .royalSlider .rsNav .rsNavSelected span.rsTmb{
    background-color: #a3397c; 
    text-shadow: 1px 1px 1px rgba(0,0,0,0.5);
    color: white;
}

.articulos .royalSlider li.tab ul li:hover{
  background: white;
}

.articulos .royalSlider li.tab ul li{
  width: 300px;
  overflow: hidden;
  float: left;
  padding: 0px 6px 10px 6px;
  margin: 10px 1px 1px 1px;
}

.articulos .royalSlider li.tab ul li:nth-child(3n){
  padding: 0px 7px 10px 6px;
}

.articulos .royalSlider li.tab ul li h3{
  color: #555555;
  text-shadow: 0px 0px 1px rgba(0,0,0,0.5);
  font-size: 16px;
  margin: 8px;
  height: 22px;
  overflow: hidden;
}

.articulos .royalSlider li.tab ul li p{
  color: #636363;
  margin: 8px;
  height: 33px;
  overflow: hidden;
}

.articulos .royalSlider li.tab ul li img{
  width: 300px;
  height: 192px;
}


.articulos .royalSlider li.tab li .social .num,
.articulos .royalSlider li.tab li .social .social-stub{
    margin: 7px 56px 0px 0px;
    float: left;
}

.articulos .royalSlider li.tab li:hover .social .num,
.articulos .royalSlider li.tab li .social .social-stub{
    display: none;
}

.articulos .royalSlider li.tab li .social .num,
.articulos .royalSlider li.tab li:hover .social .social-stub{
    display: block;
}



.articulos .royalSlider .rsNav span.rsTmb i.icon.arrow:before{
    content: "\e011";
    color: #b3468e;
}

.articulos .royalSlider .rsNav .rsNavSelected span.rsTmb i.icon.arrow:before{
    content: "\e037";
    color: #912a6a;
}


/*TABLET*/

.tablet .articulos .royalSlider .rsNav .rsTab:nth-child(n+6),
.tablet .articulos .royalSlider li.tab ul li:nth-child(n+5){
    display: none;
}

.tablet .articulos > .royalSlider .rsNav
{
    width: 628px;
}


.tablet .articulos .royalSlider li.tab ul li {
  padding: 10px 4px 10px 6px;
}

/*
MOBILE
*/
  
.mobile .articulos .royalSlider li.tab ul li,
.mobile .articulos,
.mobile .articulos .royalSlider{
    min-width: 320px;
    width: 100%;
}

.mobile .articulos .royalSlider li.tab ul li h3{
  height: 80px;
}


.mobile .articulos .royalSlider li.tab ul li{
  padding: 10px 6px 10px 6px;
  margin: 1px 1px 1px 1px;
}

.mobile .articulos .royalSlider li.tab ul li:nth-child(3n),
.mobile .articulos .royalSlider li.tab ul li .social,
.mobile .articulos .royalSlider li.tab ul li i.icono,
.mobile .articulos .royalSlider li.tab ul li div.categoria{
    display: none;
}

.mobile .articulos > .royalSlider .rsNav{
    overflow-x:auto;
    overflow-y:hidden;
    height: 32px;
}

.mobile .articulos .royalSlider li.tab ul li img{
  width: 120px;
  height: 80px;
  float: left;
}

.mobile .articulos .royalSlider .rsNav span.rsTmb{
    font-size: 12px;
    padding: 7px 6px 6px 6px;
}

.mobile .articulos .royalSlider li.tab div.contenido{
  position: absolute;
  margin: 0px 0px 0px 122px;
}

.mobile .articulos .royalSlider li.tab  h3{
  margin: 0px 0px 0px 8px;
  font-size: 13px;
}

.mobile .articulos .royalSlider li.tab ul li p{
  margin: 0px 0px 0px 8px;
  line-height: normal;
  height: 51px;
  overflow: hidden;
  font-size: 12px;
  display: none;
}


.mobile .articulos .royalSlider li.tab ul li:first-child{
  padding: 0px;
  margin: 0px;
  position: relative;
  left: 0px;
}

.mobile .articulos .royalSlider li.tab ul li:first-child img {
  width: 100%;
  height: auto;
}

.mobile .articulos .royalSlider li.tab ul li:first-child div.contenido {
  overflow: hidden;
  clear: both;
  padding: 10px 10px 10px 10px;
  margin: 0px;
  bottom: 0px;
  height: 60px;
}

.mobile .articulos .royalSlider li.tab ul li:first-child h3{
  color: white;
  font-family: Ubuntu;
  text-decoration: none;
  z-index: 1;
  text-shadow: 4px 4px 4px rgba(0,0,0,0.5);
  font-weight: bold;
  font-size: 17px;
  margin: 5px 10px 2px 0px;
  line-height: 17px;
}

.mobile .articulos .royalSlider li.tab ul li:first-child span {
  height: 80px;
  width: 100%;
  position: absolute;
  bottom: 0px;
  margin: 0px;
  background-color: #020202;
  opacity: 0.6;
  display: block;
}

.mobile .articulos .royalSlider li.tab ul li:first-child p {
  color: white;
  z-index: 1;
  text-decoration: none;
  bottom: 0px;
  margin: 0px;
  opacity: 0.8;
  height: 35px;
  overflow: hidden;
}